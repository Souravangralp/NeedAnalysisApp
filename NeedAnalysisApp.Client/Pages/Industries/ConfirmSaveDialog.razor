<MudDialog>
    <DialogContent>
        <MudText>@ContentText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton EndIcon="@Icons.Material.Outlined.Cancel" Color="Color.Default" OnClick="Cancel">Cancel</MudButton>
        <MudButton EndIcon="@Icons.Material.Outlined.Save" Color="Color.Success" OnClick="UpdateRange">UpdateRange</MudButton>
    </DialogActions>
</MudDialog>
