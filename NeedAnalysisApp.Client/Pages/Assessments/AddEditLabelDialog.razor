<MudDialog>
    <DialogContent>
        <MudContainer>
            <MudTextField @bind-Value="Label.Value" Label="Label" Variant="Variant.Text"></MudTextField>
        </MudContainer>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="Submit">Save</MudButton>
    </DialogActions>
</MudDialog>